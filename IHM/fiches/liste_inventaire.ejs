<section class="modal modalfiche">
</section>
<section class="modal modalconfirmation">
    <div class="confirmation">
        <div class="titreconfirmation">
            <div class="titre">Confirmation de désactivation </div>
            <div class="btnclose">&#10005;</div>

        </div>
        <div class="corpsconfirmation">
            <div class="messageconfirm">
                <div class="message">Confirmez-vous la désactivation (O/N) ?</div>
            </div>
            <div class="dataconfirm"></div>
        </div>
        <div class="piedconfirmation">
            <button class="btnappli btnoui">Oui</button>
            <button class="btnappli btnnon">Non</button>
        </div>
    </div>
