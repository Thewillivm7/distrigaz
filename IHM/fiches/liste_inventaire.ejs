<section class="listeinventaire">
    <section class="titrepage">
        <div class="titre">Gestion des inventaires de stock</div>
        <button class="btnappli" id="btnnew">Nouvel inventaire</button>
        <div class="closeliste">&#10005;</div>
    </section>
    <section class="zonefiltre">
        <img src="assets/icones/iconepage/loupe.png" alt="">
        <input type="text" name="" id="filtredata">
        <span id="clearfilter">&#10005;</span>
    </section>
    <section class="tablist">
        <section class="headlist">
            <div class="colhead">N° inventaire</div>
            <div class="colhead">Libellé</div>
            <div class="colhead">Statut</div>
