<link rel="stylesheet" href="css/fiche_commercial.css">
<script src="js/fiches/fiche_commercial.js"></script>
<script>
    var infoFiche = <%- JSON.stringify(infoFiche) %>;
</script>
<section class="fiche fichecom">
    <section class="zonetitre">
        <section class="titrepage">
            <div class="titre"></div>
            <div class="elcour"></div>
        </section>
        <span class="closefiche">&#10005;</span>
    </section>
    <section class="zonecorps">
        <section class="section section1">
            <div class="grdata">
                <label for="">Code</label>
                <input type="text" class="codetable" id="codecom" disabled>
            </div>
            <div class="grdata">
                <label class="requis" for="">Type</label>
                <select name="" id="listetype">
                        <option value="1">Agent OLIVE</option>
                        <option value="2">Apporteur d'affaires</option>
                    </select>
            </div>
        </section>
        <section class="section section2">
            <div class="grdata">
                <label class="requis" for="">Civilité</label>
                <select name="" id="listecivilite">
                        <option value="1">M.</option>
                        <option value="2">Mme</option>
                        <option value="3">Mlle</option>
                    </select>
            </div>
            <div class="grdata">
                <label class="requis" for="">Nom</label>
                <input type="text" id="nomcom" autocomplete="off">
            </div>
            <div class="grdata">
                <label class="requis" for="">Prénoms</label>
                <input type="text" id="prenomcom" autocomplete="off">
            </div>
        </section>
        <section class="section section3">
            <div class="grdata">
                <label class="requis" for="">Téléphone</label>
                <input type="text" name="" id="telcom" autocomplete="off">
            </div>
            <div class="grdata">
                <label for="">Email</label>
                <input type="email" name="" id="emailcom" autocomplete="off">
            </div>
        </section>
    </section>
    <div class="zonebtn">
        <button class="btnappli" id="btnannul">Annuler</button>
        <button class="btnappli" id="btnenr" disabled>Enregistrer</button>
    </div>
</section>