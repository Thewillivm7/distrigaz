<link rel="stylesheet" href="css/fiche_entrestock.css">
<script defer src="js/fiches/fiche_entrestock.js"></script>
<script>
    var infoFiche = <%- JSON.stringify(infoFiche) %>;
</script>
<section class="fiche ficheentreestock">
    <section class="zonetitre">
        <section class="titrepage">
            <div class="titre"></div>
            <div class="elcour"></div>
        </section>
        <span class="closefiche">&#10005;</span>
    </section>
    <section class="zonecorps">
        <section class="entfiche">
            <section class="section section1">
                <div class="grdata zonedepot">
                    <label for="">Dépôt : </label>
                    <div id="depot"></div>
                </div>
                <div class="grdata zoneobjet">
                    <label for="">Objet de l'entrée de stock </label>
                    <textarea name="" id="objetentreestock" >Entrée de produits</textarea>
                </div>
            </section>
            <section class="section section2">
                <div class="grsec zonenum">
                    <label for="">Entree N°</label>
                    <span id="numentreestock"></span>
                </div>
                <div class="grsec zonedate">
                    <label for="">Date : </label>
                    <input type="date" name="" id="dateentreestock">
                </div>
              
            </section>
        </section>
        <section class="tablist">
            <section class="headlist">
                <div class="colhead">#</div>
                <div class="colhead">Article</div>
                <div class="colhead">Qté</div>
                <div class="colhead">Prix unitaire</div>
                <div class="colhead">Total HT</div>
                <button class="btnaddart">&#10010</button>
            </section>
            <section class="bodylist"></section>
        </section>
       
    </section>
    <div class="zonebtn">
        <button class="btnappli" id="btnannul">Annuler</button>
        <button class="btnappli" id="btnenr" disabled>Enregistrer</button>
    </div>
</section>